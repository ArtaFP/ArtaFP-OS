; boot.asm
section .multiboot_header
header_start:
    dd 0xe85250d6                ; Magic number (Multiboot 2)
    dd 0                         ; Architecture (0 = x86)
    dd header_end - header_start  ; Header length
    dd 0x100000000 - (0xe85250d6 + 0 + (header_end - header_start))  ; Checksum
header_end: